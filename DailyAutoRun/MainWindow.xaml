<Window x:Class="DailyAutoRun.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DailyAutoRun"
        mc:Ignorable="d"
        Title="DailyRun" Height="550" Width="1200">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="77*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Attendance" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="Attendance_Click" Height="50" Width="150" Grid.ColumnSpan="2"/>
        <Button Content="GradeBook" HorizontalAlignment="Left" Margin="10,73,0,0" VerticalAlignment="Top" Click="GradeBook_Click" Height="50" Width="150" Grid.ColumnSpan="2"/>
        <Button Content="Hello Word!" HorizontalAlignment="Left" Margin="10,141,0,0" VerticalAlignment="Top" Click="HelloWorld_Click" Height="50" Width="150" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
